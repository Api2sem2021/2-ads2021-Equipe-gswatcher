oKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCgoKCmRpdi5jaGFydC13cmFwcGVyIHsKICBkaXNwbGF5OiBmbGV4OwogIGFsaWduLWl0ZW1zOiBjZW50ZXI7CiAganVzdGlmeS1jb250ZW50OiBjZW50ZXI7Cn0K"},{"version":3,"sources":["DevDetails.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4HA;AACA;AACA;AACA;AACA","file":"DevDetails.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div>\n    <h1>DEV Robinho</h1>\n    <v-container>\n      <h3>Tasks per Project</h3>\n      <v-divider></v-divider>\n      <barDevProjs />\n    </v-container>\n    <v-container>\n      <!-- Visualização dos projetos em cards-->\n      <h3>Projects</h3>\n      <v-divider></v-divider>\n      <!-- Botões de ordenação -->\n      <v-container class=\"my-5\">\n        <v-layout row class=\"mb-3\">\n          <v-btn small flat color=\"grey\" @click=\"sortBy('nome')\">\n            <v-icon left small>mdi-folder</v-icon>\n            <span class=\"caption text-lowercase\">Project</span>\n          </v-btn>\n          <v-btn small flat color=\"grey\" @click=\"sortBy('total')\">\n            <v-icon left small>mdi-list-status</v-icon>\n            <span class=\"caption text-lowercase\">Total Tasks</span>\n          </v-btn>\n          <v-btn small flat color=\"grey\" @click=\"upload(projects)\">\n            <v-icon left small>mdi-timer</v-icon>\n            <span class=\"caption text-lowercase\">Total Hours</span>\n          </v-btn>\n          <v-btn small flat color=\"grey\" @click=\"sortBy('id')\">\n            <v-icon left small>mdi-calendar</v-icon>\n            <span class=\"caption text-lowercase\">Started At</span>\n          </v-btn>\n        </v-layout>\n      </v-container>\n      <!-- Cards dos projetos -->\n      <v-container>\n        <v-row>\n          <v-col v-for=\"project in projs\" :key=\"project.id\">\n            <v-flex>\n              <v-layout row wrap>\n                <v-flex xs12 lg12>\n                  <v-card\n                    :loading=\"loading\"\n                    :class=\"`mx-auto my-4 project ${project.id}`\"\n                    width=\"420\"\n                  >\n                    <v-card-title class=\"title\">{{\n                      project.nome\n                    }}</v-card-title>\n                    <v-card-text>\n                      <div>\n                        <span>Total Tasks: {{ project.total }}</span>\n                      </div>\n                      <div>\n                        <span>Total Hours: {{ project.total }}</span>\n                      </div>\n                      <div>\n                        <span>Start Date: {{ project.total }}</span>\n                      </div>\n                    </v-card-text>\n                    <v-card-actions class=\"my-0\">\n                      <v-btn :to=\"`/about/${project.id}`\" outlined text>\n                        Details\n                        <v-icon>mdi-google-analytics</v-icon>\n                      </v-btn>\n                    </v-card-actions>\n                  </v-card>\n                </v-flex>\n              </v-layout>\n            </v-flex>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport DataService from \"../services/DataService\";\nimport barDevProjs from \"@/components/base/barDevProjs\";\n\nexport default {\n  name: \"DevDetails\",\n  components: {\n    barDevProjs,\n  },\n  data: function () {\n    return {\n      projs: [],\n    };\n  },\n  methods: {\n    log(data) {\n      console.log(\"data\");\n      console.log(data.id);\n    },\n    sortBy(prop) {\n      this.projs.sort((a, b) => (a[prop] < b[prop] ? -1 : 1));\n      console.log(this.projs);\n    },\n    retrieveProjs() {\n      DataService.getAllProjs()\n        .then((response) => {\n          this.projs = response.data.map(this.getDisplayProjs);\n          // console.log(response.data);\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n    getDisplayProjs(proj) {\n      return {\n        id: proj.projeto_id,\n        nome: proj.projeto_nome,\n        total: proj.total_de_task,\n      };\n    },\n  },\n  mounted() {\n    this.retrieveProjs();\n  },\n};\n</script>\n\n<style scoped>\ndiv.chart-wrapper {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n</style>"]}]}