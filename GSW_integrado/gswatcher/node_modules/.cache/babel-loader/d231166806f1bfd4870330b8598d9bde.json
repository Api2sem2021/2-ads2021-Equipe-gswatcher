sult":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Home.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyFA,OAAA,YAAA,MAAA,gCAAA;AACA,OAAA,WAAA,MAAA,yBAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,MADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,YAAA,EAAA;AADA,GAFA;AAMA,EAAA,IAAA,EAAA,gBAAA;AACA,WAAA;AACA,MAAA,KAAA,EAAA;AADA,KAAA;AAGA,GAVA;AAWA,EAAA,OAAA,EAAA;AACA,IAAA,GADA,eACA,IADA,EACA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,EAAA;AACA,KAJA;AAKA,IAAA,MALA,kBAKA,IALA,EAKA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA;AAAA,eAAA,CAAA,CAAA,IAAA,CAAA,GAAA,CAAA,CAAA,IAAA,CAAA,GAAA,CAAA,CAAA,GAAA,CAAA;AAAA,OAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,KAAA;AACA,KARA;AASA,IAAA,aATA,2BASA;AAAA;;AACA,MAAA,WAAA,CAAA,WAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,GAAA,CAAA,KAAA,CAAA,eAAA,CAAA,CADA,CAEA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAPA;AAQA,KAlBA;AAmBA,IAAA,eAnBA,2BAmBA,IAnBA,EAmBA;AACA,aAAA;AACA,QAAA,EAAA,EAAA,IAAA,CAAA,UADA;AAEA,QAAA,IAAA,EAAA,IAAA,CAAA,YAFA;AAGA,QAAA,KAAA,EAAA,IAAA,CAAA;AAHA,OAAA;AAKA;AAzBA,GAXA;AAsCA,EAAA,OAtCA,qBAsCA;AACA,SAAA,aAAA;AACA;AAxCA,CAAA","sourcesContent":["<template>\n  <div class=\"home\">\n    <h1>Tasks Per Project</h1>\n    <v-divider></v-divider>\n    <v-container class=\"my-5\">\n      <v-layout row class=\"mb-3 sb-1\"> </v-layout>\n      <mainBarChart />\n      <v-layout> </v-layout>\n    </v-container>\n\n    <h1>My Projects</h1>\n    <v-divider></v-divider>\n    <v-container class=\"my-5\">\n      <v-layout row class=\"mb-3\">\n        <v-btn small flat color=\"grey\" @click=\"sortBy('nome')\">\n          <v-icon left small>mdi-folder</v-icon>\n          <span class=\"caption text-lowercase\">Project</span>\n        </v-btn>\n        <!-- <v-btn small flat color=\"grey\" @click=\"sortBy('id')\">\n          <v-icon left small>mdi-calendar</v-icon>\n          <span class=\"caption text-lowercase\">Started At</span>\n        </v-btn> -->\n        <v-btn small flat color=\"grey\" @click=\"sortBy('total')\">\n          <v-icon left small>mdi-list-status</v-icon>\n          <span class=\"caption text-lowercase\">Total Tasks</span>\n        </v-btn>\n        <!-- <v-btn small flat color=\"grey\" @click=\"upload(projects)\">\n            <v-icon left small>mdi-upload</v-icon>\n            <span class=\"caption text-lowercase\">Upload</span>\n          </v-btn> -->\n      </v-layout>\n    </v-container>\n\n    <v-spacer></v-spacer>\n\n    <v-container>\n      <v-row>\n        <!--ALTERAÇÃO NO COLS-->\n        <v-col v-for=\"project in projs\" :key=\"project.id\">\n          <!-- Arrumar o tamanho dos Cards -->\n          <v-flex>\n            <!--xs6 sm4 md6---------------------------------------------------------------------->\n            <v-layout row wrap>\n              <v-flex xs12 lg12>\n                <v-card\n                  :loading=\"loading\"\n                  :class=\"`mx-auto my-4 project ${project.id}`\"\n                  width=\"420\"\n                >\n                  <v-responsive>\n                    <!-- <v-img :src=\"project.user.avatar\" height=\"120\"></v-img> -->\n                  </v-responsive>\n\n                  <v-card-title class=\"title\">{{ project.nome }}</v-card-title>\n                  <v-card-text>\n                    <div class=\"my-0 subtitle-1\">\n                      <!--ALTERAÇÃO-->\n                      <p>||</p>\n                    </div>\n                    <div>Total Tasks: {{ project.total }}</div>\n                  </v-card-text>\n                  <v-card-actions class=\"my-0\">\n                    <v-btn :to=\"`/about/${project.id}`\" outlined text>\n                      <!-- ROTA DE TESTE  -->\n                      <!-- /about - carrega depois de alterar a página -->\n                      Details\n                      <v-icon>mdi-google-analytics</v-icon>\n                    </v-btn>\n\n                    <!-- <v-btn :to=\"`/projeto/${project.id}`\" outlined text> -->\n                    <!-- ROTA DE TESTE  -->\n                    <!-- /projeto - carrega antes de alterar a página -->\n                    <!-- Details -->\n                    <!-- <v-icon>mdi-close-octagon</v-icon> -->\n                    <!-- </v-btn>      -->\n                  </v-card-actions>\n                </v-card>\n              </v-flex>\n            </v-layout>\n\n            <!---------------------------------------------------------------------------------->\n          </v-flex>\n        </v-col>\n      </v-row>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport mainBarChart from \"@/components/base/mainBarChart\";\nimport DataService from \"../services/DataService\";\n\nexport default {\n  name: \"Home\",\n  components: {\n    mainBarChart,\n  },\n\n  data: function () {\n    return {\n      projs: [],\n    };\n  },\n  methods: {\n    log(data) {\n      console.log(\"data\");\n      console.log(data.id);\n    },\n    sortBy(prop) {\n      this.projs.sort((a, b) => (a[prop] < b[prop] ? -1 : 1));\n      console.log(this.projs);\n    },\n    retrieveProjs() {\n      DataService.getAllProjs()\n        .then((response) => {\n          this.projs = response.data.map(this.getDisplayProjs);\n          // console.log(response.data);\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n    getDisplayProjs(proj) {\n      return {\n        id: proj.projeto_id,\n        nome: proj.projeto_nome,\n        total: proj.total_de_task,\n      };\n    },\n  },\n  mounted() {\n    this.retrieveProjs();\n  },\n};\n</script>\n<style >\n</style>"],"sourceRoot":"src/views"}]}