ht=\"350\"\n        width=\"100%\"\n        :options=\"chartOptions\"\n        :series=\"series\"\n      ></apexchart>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"barChart\",\n  data() {\n    return {\n      series: [\n        {\n          data: [13, 12, 11, 20, 94, 21, 9],\n        },\n      ],\n      // array 'series' trata dos valores de cada status dentro do projeto\n      //\"FOR_TEST\",\"DONE\",\"RELEASE_TO_PROD\",\"QA_TESTING\",\"IN_PROGRESS\",\"PROD_DEPLOYING\",\"QA_DEPLOYING\",\n      chartOptions: {\n        chart: {\n          width: \"100%\",\n          height: \"auto\",\n          type: \"bar\",\n        },\n        plotOptions: {\n          bar: {\n            borderRadius: 4,\n            dataLabels: {\n              position: \"top\", // top, center, bottom\n            },\n          },\n        },\n        dataLabels: {\n          enabled: true,\n          formatter: function (val) {\n            return val; /*original: + \" tasks\"*/\n          },\n          offsetY: -20,\n          style: {\n            fontSize: \"12px\",\n            colors: [\"#1976d2\"],\n          },\n        },\n\n        xaxis: {\n          categories: [\n            \"FOR_TEST\",\n            \"DONE\",\n            \"RELEASE_TO_PROD\",\n            \"QA_TESTING\",\n            \"IN_PROGRESS\",\n            \"PROD_DEPLOYING\",\n            \"QA_DEPLOYING\",\n          ],\n          position: \"bottom\",\n          axisBorder: {\n            show: false,\n          },\n          axisTicks: {\n            show: false,\n          },\n          crosshairs: {\n            fill: {\n              type: \"gradient\",\n              gradient: {\n                colorFrom: \"#D8E3F0\",\n                colorTo: \"#BED1E6\",\n                stops: [0, 100],\n                opacityFrom: 0.4,\n                opacityTo: 0.5,\n              },\n            },\n          },\n          tooltip: {\n            enabled: true,\n          },\n        },\n        yaxis: {\n          axisBorder: {\n            show: false,\n          },\n          axisTicks: {\n            show: false,\n          },\n          labels: {\n            show: false,\n            formatter: function (val) {\n              return val;\n            },\n          },\n        },\n        //title: {\n        //text: \"Tasks Per Status\",\n        //floating: true,\n        //   offsetY: 330,\n        //offsetY: -5,\n        //align: \"left\",\n        //style: {\n        //color: \"#444\",\n        //fontSize: \"15px\",\n        //},\n        //},\n        colors: [\"#304758\"],\n      },\n    };\n  },\n};\n</script>\n\n\n<style>\ndiv.chart-wrapper {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n</style>"],"sourceRoot":"src/components/base"}]}